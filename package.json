        "react-dom": "^16.4.0",
        "react-helmet": "^5.2.1",
        "react-i18next": "^8.3.8",
        "react-redux": "^5.0.7",
        "redux": "^4.0.0",
        "redux-persist": "^5.10.0",
        "redux-saga": "^1.0.5"
    },
    "devDependencies": {
        "babel": "^6.23.0",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-eslint": "^10.0.1",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-0": "^6.5.0",
        "css-loader": "^3.2.0",
        "eslint": "^5.9.0",
        "eslint-plugin-babel": "^5.3.0",
        "eslint-plugin-react": "^7.11.1",
        "node-sass-chokidar": "^1.3.0",
        "npm-run-all": "^4.1.3",
        "react-router-dom": "^4.3.1",
        "react-scripts": "^1.1.4",
        "sass": "^1.9.0",
        "source-map-explorer": "^2.0.1"
    },
    "scripts": {
        "start-server": "react-scripts start & sass --watch --no-source-map src:src",
        "sass-watch": "sass --watch --no-source-map src:src",
        "build": "npm run build-css && react-scripts build stage-0",
        "build-css": "node-sass-chokidar src/ -o src/",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "deploy": "gh-pages -d build",
        "analyze": "source-map-explorer 'build/static/js/*.js'",
        "start": "npm-run-all -p watch-css start-js",
        "start-js": "PORT=1990 react-scripts start",
        "gulp": "gulp",
        "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
        "lint": "./node_modules/.bin/eslint src",
        "lint-fixup": "./node_modules/.bin/eslint src --ext .js,.jsx --fix"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
